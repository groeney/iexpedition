<hr class="details-line with_arrow">
<h5>Address Details</h5>
<div class="row">
  <div class="col-md-6">
    <%= a.input :unit %>
  </div>
  <div class="col-md-6">
    <%= a.input :street %>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <%= a.input :city %>
  </div>
  <div class="col-md-6">
    <%= a.input :postcode %>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <%= a.input :state %>
  </div>
  <div class="col-md-6">
    <div class="input select">
      <label style="display:block;" for="user_dob">Country</label>
      <%= a.country_select :country, { include_blank: "Select Country" } %>
    </div>
  </div>
</div>
