<!-- Locals to pass to partial: selector_id, min_value, max_value, min_label, max_label, default_value, filter_name -->
<li>
  <div class="title">
    <span><%= filter_name %></span>
  </div>
  <%= render partial: "includes/search/slider_filter_item", locals: { selector_id: selector_id, min_value: min_value, max_value: max_value, min_label: min_label, max_label: max_label, default_value: default_value } %>
</li>